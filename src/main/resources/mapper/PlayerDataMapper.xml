<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.demo.mapper.PlayerDataMapper">
    <select id="getPlayerDataById" resultType="PlayerData" parameterType="int">
        select * from player_data where p_id = #{pId}
    </select>
</mapper>